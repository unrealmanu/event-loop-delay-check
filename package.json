{
    "name": "event-loop-delay-check",
    "version": "0.0.1",
    "description": "Check the event loop delay",
    "repository": "https://github.com/unrealmanu/event-loop-delay-check",
    "license": "MIT",
    "author": "Manuel Trebbi <unrealmanumaster@gmail.com>",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "yarn lint && yarn run build:clean && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
        "build:clean": "rimraf ./dist/",
        "create:index": "cti ./src -b && yarn lint",
        "format": "prettier --write ./src/ ./test/",
        "lint": "eslint ./src/**/*.ts --fix",
        "prepare": "husky install",
        "test": "jest ./src/* --config ./jest.config.js",
        "test:coverage": "yarn test --coverage",
        "test:watch": "yarn test --watch"
    },
    "lint-staged": {
        "*": "npx prettier --write --ignore-path .gitignore --ignore-unknown",
        "*.ts": "npx eslint --fix"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^29.5.6",
        "@types/node": "^20.8.8",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "create-ts-index": "^1.14.0",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-config-standard-with-typescript": "^38.0.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-n": "^16.0.2",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-unused-imports": "^3.0.0",
        "husky": "^8.0.3",
        "jest": "29.6.4",
        "jest-extended": "^4.0.1",
        "jest-sonar": "^0.2.16",
        "lint-staged": "14.0.1",
        "prettier": "^3.0.3",
        "pretty-quick": "^3.1.3",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.7",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18.16"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "keywords": [
        "event",
        "loop",
        "delay",
        "check",
        "elu"
    ]
}
