{
    addNewline: true,
        excludes: [
            '@types',
            'typings',
            '__test__',
            '__tests__',
            'node_modules'
        ],
            fileExcludePatterns: ['.test.ts', 'spec.ts'],
                fileFirst: false,
                    globOptions: {
        dot: true,
            ignore: [
                '@types/**',
                'typings/**',
                '__test__/**',
                '__tests__/**',
                'node_modules/**',
                '__snapshots__/**'
            ],
                nonull: true,
  },
    output: 'index.ts',
        quote: "'",
            targetExts: [
                'ts',
                'tsx',
            ],
                useSemicolon: true,
                    useTimestamp: false,
                        verbose: false,
                            withoutBackupFile: false,
                                withoutComment: false,
}